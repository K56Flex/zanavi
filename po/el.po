msgid "oncoming traffic!"
msgstr ""

#. TRANSLATORS: The first argument is strength, the second direction, the third distance and the fourth destination Example: 'Turn 'slightly' 'left' in '100 m' 'onto baker street'
#: navigation.c:2576 navigation.c:3141 navigation.c:3148
#, c-format
msgid "Turn %1$s%2$s %3$s%4$s"
msgstr "Στρίψτε %1$s%2$s %3$s%4$s"

#: navigation.c:2589 navigation.c:2593 navigation.c:2598
msgid "then"
msgstr ""

#. TRANSLATORS: First argument is strength, second direction, third how many roads to skip, fourth destination
#: navigation.c:2613 navigation.c:2619 navigation.c:3179 navigation.c:3185
#, c-format
msgid "then turn %1$s%2$s %3$s%4$s"
msgstr "και στρίψτε %1$s%2$s %3$s%4$s"

#. TRANSLATORS: EXAMPLE: You have reached your destination in 300 meters
#: navigation.c:2639 navigation.c:3203
#, c-format
msgid "You have reached your destination %s"
msgstr "Φτάσατε στον προορισμό σας %s"

