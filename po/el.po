msgid "%sinto the %s%s%s|neutral form"
msgstr "%sστο %s%s%s"

#. TRANSLATORS: gives the name of the next road to turn into (into the E17)
#: navigation.c:2314
#, c-format
msgid "%sinto the %s"
msgstr "%s στην %s"

#. TRANSLATORS: right, as in 'Turn right'
#: navigation.c:2341 navigation.c:2656 navigation.c:3604
msgid "right"
msgstr "δεξιά"

#. TRANSLATORS: left, as in 'Turn left'
#: navigation.c:2428 navigation.c:2739 navigation.c:3609
msgid "left"
msgstr "αριστερά"

